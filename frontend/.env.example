# Clerk Authentication (App Router)
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY=pk_test_your_clerk_publishable_key
CLERK_SECRET_KEY=sk_test_your_clerk_secret_key

# Clerk Webhook (optional - for user events)
CLERK_WEBHOOK_SECRET=whsec_your_webhook_secret

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_KEY=your_supabase_anon_key

# Clerk Redirect Paths
NEXT_PUBLIC_CLERK_AFTER_SIGN_IN_URL=/app
NEXT_PUBLIC_CLERK_AFTER_SIGN_UP_URL=/app

# Organization Redirect Paths
NEXT_PUBLIC_CLERK_AFTER_CREATE_ORGANIZATION_URL=/app
NEXT_PUBLIC_CLERK_AFTER_LEAVE_ORGANIZATION_URL=/app

# Data Ingestion APIs (optional)
COURTLISTENER_TOKEN=your_courtlistener_token
OPENFDA_KEY=your_openfda_key
LOCAL_RSS=your_local_rss_feeds

# LiveKit Configuration (optional - for real-time voice features)
NEXT_PUBLIC_LIVEKIT_URL=wss://your-livekit-url.livekit.cloud
NEXT_PUBLIC_LIVEKIT_API_KEY=your_livekit_api_key
LIVEKIT_API_SECRET=your_livekit_api_secret

# Backend API URL
NEXT_PUBLIC_BACKEND_URL=http://localhost:8000
